{
  "builds": [
    {
      "src": "html/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "project/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/project/(.*)", "dest": "/project/$1" },
    { "src": "/(.+)$", "dest": "/html/$1" },
    { "src": "/", "dest": "/html/index.html" }
  ]
}